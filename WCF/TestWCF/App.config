<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
    <system.serviceModel>
        <bindings>
            <wsDualHttpBinding>
                <binding name="WSDualHttpBinding_ServiceB" />
            </wsDualHttpBinding>
            <wsHttpBinding>
                <binding name="WSHttpBinding_ServiceA" />
                <binding name="WSHttpBinding_ServiceC" />
            </wsHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:11223/request-reply" binding="wsHttpBinding"
                bindingConfiguration="WSHttpBinding_ServiceA" contract="ServiceReference1.ServiceA"
                name="WSHttpBinding_ServiceA">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
            <endpoint address="http://localhost:11224/duplex" binding="wsDualHttpBinding"
                bindingConfiguration="WSDualHttpBinding_ServiceB" contract="ServiceReference2.ServiceB"
                name="WSDualHttpBinding_ServiceB">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
            <endpoint address="http://localhost:11225/one-way" binding="wsHttpBinding"
                bindingConfiguration="WSHttpBinding_ServiceC" contract="ServiceReference3.ServiceC"
                name="WSHttpBinding_ServiceC">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>